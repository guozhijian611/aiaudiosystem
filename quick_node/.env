# RabbitMQ配置
RABBITMQ_HOST=10.0.0.130
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=admin
RABBITMQ_VHOST=/

# API配置
API_BASE_URL=http://10.0.0.130:8787

# VAD模型配置
VAD_MODEL=fsmn-vad
VAD_MODEL_REVISION=v2.0.4
VAD_MAX_END_SILENCE_TIME=800
VAD_MAX_START_SILENCE_TIME=3000
VAD_MIN_SPEECH_DURATION=250

# 模型缓存配置
MODEL_CACHE_DIR=./models
DISABLE_UPDATE=true

# 文件处理配置
TEMP_DIR=./temp
WORK_DIR=./work

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=./logs/quick_node.log

# 队列配置
QUEUE_FAST_PROCESS=fast_process_queue
QUEUE_TTL=3600000
QUEUE_DURABLE=true

# 其他配置
MAX_WORKERS=2
DOWNLOAD_TIMEOUT=300
PROCESS_TIMEOUT=600 