# RabbitMQ配置
RABBITMQ_HOST=10.0.0.130
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=admin
RABBITMQ_PASSWORD=admin
RABBITMQ_VIRTUAL_HOST=/

# 队列名称
QUEUE_NAME=fast_process_queue

# 后端API配置
API_BASE_URL=http://10.0.0.130:8787
API_UPLOAD_ENDPOINT=/queue/upload
API_CALLBACK_ENDPOINT=/queue/handleTaskCallback

# 工作目录配置
WORK_DIR=./work
TEMP_DIR=./temp
LOG_LEVEL=INFO

# VAD模型配置
VAD_MODEL=fsmn-vad
VAD_MODEL_REVISION=v2.0.4
VAD_MAX_END_SILENCE_TIME=800
VAD_MAX_START_SILENCE_TIME=3000
VAD_MIN_SPEECH_DURATION=250

# 模型缓存配置
MODEL_CACHE_DIR=./models
DISABLE_UPDATE=True

# 队列配置
QUEUE_TTL=3600000
QUEUE_DURABLE=true

# 其他配置
MAX_WORKERS=2
DOWNLOAD_TIMEOUT=300
PROCESS_TIMEOUT=600 

# FunASR模型路径配置
FUNASR_PATH=./FunASR 